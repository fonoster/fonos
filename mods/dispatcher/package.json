{
  "name": "@fonos/dispatcher",
  "version": "0.0.81",
  "description": "Application Router for Fonos",
  "author": "Pedro Sanders <psanders@fonoster.com>",
  "homepage": "https://github.com/fonoster/fonos#readme",
  "license": "MIT",
  "main": "dist/dispatcher",
  "types": "dist/dispatcher",
  "scripts": {
    "prebuild": "rimraf ./dist tsconfig.tsbuildinfo",
    "build": "tsc -b tsconfig.json",
    "start": "cross-env NODE_ENV=dev nodemon src/dispatcher"
  },
  "nodemonConfig": {
    "watch": [
      "../"
    ],
    "ext": "ts",
    "exec": "ts-node",
    "ignore": "**/*.d.ts"
  },
  "directories": {
    "src": "src",
    "test": "test"
  },
  "files": [
    "dist",
    "etc"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fonoster/fonos.git"
  },
  "bugs": {
    "url": "https://github.com/fonoster/fonos/issues"
  },
  "dependencies": {
    "@fonos/appmanager": "^0.0.81",
    "@fonos/core": "^0.0.81",
    "@fonos/logger": "^0.0.81",
    "@fonos/marytts": "^0.0.81",
    "@fonos/numbers": "^0.0.81",
    "@fonos/storage": "^0.0.81",
    "@fonos/tts": "^0.0.81",
    "@fonos/voice": "^0.0.81",
    "agi-node": "github:psanders/agi-node",
    "dotenv": "^8.2.0",
    "phone": "^2.4.20",
    "vm2": "^3.6.10"
  },
  "devDependencies": {
    "@types/phone": "^2.4.0",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2"
  },
  "gitHead": "8efbe2e8601b3a0618e770804a205e8584d49580"
}
